<h1 class="py-4">Posts</h1>
<table class="table">
    <tr>
    <th>Title</th>
    <th>Created at</th>
    <th>Image_url</th>
    <th>Content</th>
    </tr>
    <% @posts.each do |post| %>
        <tr>
            <td><%= post.id %></td>
            <td><%= post.title %></td>
            <td><%= post.created_at %></td>
            <td><%= image_tag post.image_url %></td>
            <td><%= post.content %></td>
        </tr>
    <% end %>
</table>

